            "\u001b[0;32m/usr/local/lib/python3.10/dist-packages/torch/nn/modules/linear.py\u001b[0m in \u001b[0;36mforward\u001b[0;34m(self, input)\u001b[0m\n\u001b[1;32m    123\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    124\u001b[0m     \u001b[0;32mdef\u001b[0m \u001b[0mforward\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mself\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0minput\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0mTensor\u001b[0m\u001b[0;34m)\u001b[0m \u001b[0;34m->\u001b[0m \u001b[0mTensor\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m--> 125\u001b[0;31m         \u001b[0;32mreturn\u001b[0m \u001b[0mF\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mlinear\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0minput\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mself\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mweight\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mself\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mbias\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m    126\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    127\u001b[0m     \u001b[0;32mdef\u001b[0m \u001b[0mextra_repr\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mself\u001b[0m\u001b[0;34m)\u001b[0m \u001b[0;34m->\u001b[0m \u001b[0mstr\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n",
            "\u001b[0;31mKeyboardInterrupt\u001b[0m: "
          ]
        }
      ],
      "source": [
        "import pandas as pd\n",
        "from transformers import pipeline\n",
        "\n",
        "from google.colab import userdata\n",
        "userdata.get('HF_TOKEN')\n",
        "\n",
        "class HerbCategorizer:\n",
        "    def __init__(self):\n",
        "        # Initialize the zero-shot classification pipeline\n",
        "        self.classifier = pipeline(\"zero-shot-classification\", model=\"facebook/bart-large-mnli\")\n",
        "\n",
        "        # Define categories and subcategories\n",
        "        # Define categories and subcategories\n",
        "        self.categories = [\"Herb\", \"Spice\", \"Superfood\", \"Traditional Medicine\", \"Food Additive\", \"Culinary Ingredient\", \"Tea\"]\n",
        "\n",
        "        self.subcategories = {\n",
        "            \"Herb\": [\"Dried Herb\", \"Herb Powder\", \"Herb Extract\", \"Fresh Herb\"],\n",
        "            \"Spice\": [\"Whole Spice\", \"Ground Spice\", \"Spice Blend\"],\n",
        "            \"Superfood\": [\"Berry\", \"Seed\", \"Grain\"],\n",
        "            \"Traditional Medicine\": [\"Chinese Medicine\", \"Ayurvedic Medicine\", \"Herbal Remedy\"],\n",
        "            \"Food Additive\": [\"Natural Sweetener\", \"Thickener\", \"Flavor Enhancer\"],\n",
        "            \"Culinary Ingredient\": [\"Vegetable Powder\", \"Fruit Powder\", \"Nut Powder\"],\n",
        "            \"Tea\": [\"Herbal Tea\", \"Green Tea\", \"Black Tea\"]\n",
        "        }\n",
        "\n",
        "    def categorize_product(self, product_name):\n",
        "        # Classify the product into a category\n",
        "        category_result = self.classifier(product_name, self.categories)\n",
        "        category = category_result['labels'][0]\n",
        "\n",
        "        # Classify the product into a subcategory\n",
        "        subcategory_result = self.classifier(product_name, self.subcategories[category])\n",
        "        subcategory = subcategory_result['labels'][0]\n",
        "\n",
        "        return category, subcategory\n",
        "\n",
        "    def extract_quantity_info(self, product_name, unit):\n",
        "        # Extract quantity and type\n",
        "        if 'กรัม' in product_name or 'กก' in product_name:\n",
        "            quantity_type = 'weight'\n",
        "            if 'กก' in product_name:\n",
        "                quantity = 1\n",
        "                quantity_unit = 'kg'\n",
        "            else:\n",
        "                quantity = int(''.join(filter(str.isdigit, product_name)))\n",
        "                quantity_unit = 'g'\n",
        "        else:\n",
        "            quantity_type = 'count'\n",
        "            quantity = 1\n",
        "            quantity_unit = unit\n",
        "\n",
        "        return quantity_type, quantity, quantity_unit\n",
        "\n",
        "    def process_data(self, df):\n",
        "        results = []\n",
        "        for _, row in df.iterrows():\n",
        "            category, subcategory = self.categorize_product(row['goodname'])\n",
        "            quantity_type, quantity, quantity_unit = self.extract_quantity_info(row['goodname'], row['goodunitname'])\n",
        "\n",
        "            results.append({\n",
        "                'PrimaryKey': row['PrimaryKey'],\n",
        "                'Custname': row['Custname'],\n",
        "                'goodname': row['goodname'],\n",
        "                'Category': category,\n",
        "                'Subcategory': subcategory,\n",
        "                'Quantity Type': quantity_type,\n",
        "                'Quantity': quantity,\n",
        "                'Quantity Unit': quantity_unit,\n",
        "                'Total Amount': row[' TotaExcludeAmnt '],\n",
        "                'ChannelCode': row['ChannelCode'],\n",
        "                'QuotationNumber': row['QuotationNumber'],\n",
        "                'Senddate': row['Senddate']\n",
        "            })\n",
        "\n",
        "        return pd.DataFrame(results)\n",
        "\n",
        "# Usage example\n",
        "if __name__ == \"__main__\":\n",
        "    # Load your data\n",
        "    df = pd.read_csv('merged_sales_data.csv')\n",
        "\n",
        "    # Initialize the categorizer\n",
        "    categorizer = HerbCategorizer()\n",
        "\n",
        "    # Process the data\n",
        "    result_df = categorizer.process_data(df)\n",
        "\n",
        "    # Display the results\n",
        "    print(result_df)\n"
      ]
    },
